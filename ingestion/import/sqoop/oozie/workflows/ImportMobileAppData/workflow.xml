              <arg>&#39;\&quot;&#39;</arg>
              <arg>--split-by</arg>
              <arg>FLIGHT_NO_AND_DATE</arg>
              <arg>--null-string</arg>
              <arg>&#39;\\N&#39;</arg>
              <arg>--null-non-string</arg>
              <arg>&#39;\\N&#39;</arg>
              <arg>--class-name</arg>
              <arg>TBL_AIRCRAFT_CONF</arg>
              <arg>--compress</arg>
              <arg>--compression-codec</arg>
