            <job-tracker>${jobTracker}</job-tracker>
            <name-node>${nameNode}</name-node>
              <arg>import</arg>
              <arg>-D</arg>
              <arg>mapred.child.java.opts=&quot;-Djava.security.egd=file:/dev/../dev/urandom&quot;</arg>
              <arg>--options-file</arg>
              <arg>${MOB_SQOOP_OPTIONS_FILE}</arg>
              <arg>--query</arg>
              <arg>&quot;select DEVICE_ID, STATUS, APP_USER_TYPE_ID, APP_TYPE_ID, MODEL_TYPE, MEMBER_ID, LOCALE, CREATED_DATE, UPDATED_DATE, BATCH_ID, SYNC_ATTEMPT_COUNT, ZONE, SYNC_STATUS, API_VERSION, SKYWARDS_PAX_NAME, IS_APPLEWATCH from tbl_device_settings where updated_date between TO_DATE(&#39;${wf:actionData(&#39;shell-a0d6&#39;)[&#39;START_DATE&#39;]}&#39;,&#39;YYYY-MM-DD HH24:MI:SS&#39;) AND  TO_DATE(&#39;${wf:actionData(&#39;shell-a0d6&#39;)[&#39;END_DATE&#39;]}&#39;,&#39;YYYY-MM-DD HH24:MI:SS&#39;)  and $CONDITIONS&quot;</arg>
              <arg>--as-avrodatafile</arg>
              <arg>--map-column-java</arg>
              <arg>CREATED_DATE=String,UPDATED_DATE=String,SYNC_STATUS=String</arg>
              <arg>--enclosed-by</arg>
              <arg>&#39;\&quot;&#39;</arg>
              <arg>--split-by</arg>
              <arg>MEMBER_ID</arg>
              <arg>--null-string</arg>
              <arg>&#39;\\N&#39;</arg>
              <arg>--null-non-string</arg>
              <arg>&#39;\\N&#39;</arg>
              <arg>--class-name</arg>
              <arg>TBL_DEVICE_SETTINGS</arg>
              <arg>--compress</arg>
              <arg>--compression-codec</arg>
              <arg>org.apache.hadoop.io.compress.SnappyCodec</arg>
              <arg>--target-dir</arg>
              <arg>${PATH_TBL_DEVICE_SETTINGS}</arg>
            <file>${MOB_SQOOP_OPTIONS_FILE}#${MOB_SQOOP_OPTIONS_FILE}</file>
        </sqoop>
        <ok to="join-92aa"/>
        <error to="email-98ee"/>
    </action>
    <action name="sqoop-fc7a">
        <sqoop xmlns="uri:oozie:sqoop-action:0.2">
