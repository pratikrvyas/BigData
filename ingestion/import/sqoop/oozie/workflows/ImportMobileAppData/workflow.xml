              <arg>--target-dir</arg>
              <arg>${PATH_TBL_AIRCRAFT_CONF}</arg>
            <file>${MOB_SQOOP_OPTIONS_FILE}#${MOB_SQOOP_OPTIONS_FILE}</file>
